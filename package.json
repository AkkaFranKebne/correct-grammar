{
  "name": "grammar-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "postinstall": "npm run prisma:generate",
    "pull-sb-components": "storyblok pull-components --space 328152 --region eu",
    "generate-sb-types": "storyblok generate-typescript-typedefs --sourceFilePaths ./components.328152.json --destinationFilePath ./component-types-sb.d.ts"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.8",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@storyblok/react": "^4.4.0",
    "@tailwindcss/typography": "^0.5.16",
    "ai": "^4.0.18",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.468.0",
    "next": "15.1.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.5",
    "nodemailer": "^6.9.16",
    "openai": "^4.76.3",
    "openai-edge": "^1.2.2",
    "prisma": "^6.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.17",
    "eslint": "^9.17.0",
    "ts-node": "^10.9.1"
  }
}
